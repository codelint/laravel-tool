{
    "name": "codelint/laravel-tool",
    "description": "tool to help developing on laravel",
    "require": {
        "laravel/framework": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "homepage": "http://www.inotseeyou.com/",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "codelint",
            "email": "codelint@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Codelint\\": "src/"
        },
        "files": []
    },
